<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="496.0" prefWidth="901.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.ap.apjavafx.controller.controllerListeLieux">
   <children>
      <Text fx:id="txtTitle" layoutX="232.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Liste des lieux proposés par ">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Button fx:id="btnQuitter" layoutX="31.0" layoutY="14.0" mnemonicParsing="false" onAction="#OnQuitter">
         <graphic>
            <ImageView fitHeight="38.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/back.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <TableView fx:id="tableListeLieux" layoutX="29.0" layoutY="86.0" prefHeight="376.0" prefWidth="216.0">
        <columns>
          <TableColumn fx:id="collListeLieux" prefWidth="215.0" text="Liste des lieux" />
        </columns>
      </TableView>
      <TableView fx:id="tableListeReservation" layoutX="297.0" layoutY="86.0" prefHeight="376.0" prefWidth="299.0">
         <columns>
            <TableColumn fx:id="collListeDesReservations" prefWidth="297.0" text="Listes des reservations pour la salle" />
         </columns>
      </TableView>
      <Text fx:id="txtInformatif" layoutX="48.0" layoutY="82.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Cliquez sur un lieu pour plus de détails">
         <font>
            <Font size="10.0" />
         </font>
      </Text>
      <ImageView fitHeight="13.0" fitWidth="16.0" layoutX="31.0" layoutY="72.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../image/information.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
